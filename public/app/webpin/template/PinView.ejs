<% var i=0; %>
<% _.each(list, function(item){ %>
    <% if (i%4 === 0) { %>
    <div class="row">
    <% } %>
        <div class="col-sm-3">
            <img class="thumbnail" src="<%= item.img %>" style="width:128px;height:128px;"></img>
            <a href="<%= item.pin %>"><%= item.name %></a>
        </div>
    <% if (i%4 === 3){ %>
    </div>
    <% } %>
<% ;i++;}) %>




